		<queryString>
			<![CDATA[SELECT
  sum(base.vol_kilo) kilo,
  sum(base.vol_dollar) dinero,
  base.comm_type tipo,
  base.year_sk anio,
  base.ctr_desc,
  base.ctr_id,
  base.prod_id,
  base.prod_desc,
  tasakilo.tasa_crecimiento tasa_crecimiento_kilo,
  tasadollar.tasa_crecimiento tasa_crecimiento_dollar
from (
SELECT
  commerce.vol_kilo ,
  commerce.vol_dollar ,
  commerce.comm_type ,
  year.year_sk ,
  country.ctr_desc,
  country.ctr_id,
  product.prod_id,
  product.prod_desc
FROM
  public.commerce_fact commerce,
  public.year_dim year,
  public.product_dim product,
  public.country_dim country

WHERE
  commerce.prod_sk = product.prod_sk AND
  commerce.ctr_sk = country.ctr_sk AND
  commerce.year_sk = year.year_sk
  AND country.ctr_id in ($P!{countries})
  AND commerce.year_sk between $P!{initialDate} and $P!{finalDate}
  and product.prod_id in ($P!{products})
) base left outer join
(select prod_id,
	prod_desc,
	ctr_id,
	ctr_desc,
	case
		when tipo = 'ExportKilo' then
			'E'
		when tipo = 'ImportKilo' then
			'I'
	end tipo,
	initial_year || ' - ' || final_year periodo,
	initial_year,
	final_year,
	initial_value,
	final_value,
	tasa_crecimiento*100 tasa_crecimiento
 from getTasaCrecimientoCommProd('ExportKilo,ImportKilo',$P{products},$P{countries},'$P!{initialDate}-$P!{finalDate}')
) tasakilo on   tasakilo.ctr_id = base.ctr_id
  and tasakilo.prod_id = base.prod_id
  and tasakilo.tipo = base.comm_type
  and base.year_sk between tasakilo.initial_year and tasakilo.final_year
left outer join
(select prod_id,
	prod_desc,
	ctr_id,
	ctr_desc,
	case
		when tipo = 'ExportDollar' then
			'E'
		when tipo = 'ImportDollar' then
			'I'
	end tipo,
	initial_year || ' - ' || final_year periodo,
	initial_year,
	final_year,
	initial_value,
	final_value,
	tasa_crecimiento*100 tasa_crecimiento
 from getTasaCrecimientoCommProd('ExportDollar,ImportDollar',$P{products},$P{countries},'$P!{initialDate}-$P!{finalDate}')
) tasadollar on   tasadollar.ctr_id = base.ctr_id
  and tasadollar.prod_id = base.prod_id
  and tasadollar.tipo = base.comm_type
  and base.year_sk between tasadollar.initial_year and tasadollar.final_year

group by base.comm_type,
  base.year_sk,
  base.ctr_desc,
  base.ctr_id,
  base.prod_id,
  base.prod_desc,
  tasakilo.tasa_crecimiento,
  tasadollar.tasa_crecimiento]]>
		</queryString>
